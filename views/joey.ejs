<!DOCTYPE html>
<html>
<body>
<div>
    <input id="a">
    <input id="b">
    <button type="button" onclick='add()'>Add</button>
    <button type="button" onclick='subtract()'>Subtract</button>

    <div id="result">2</div>

</div>
<script>
    let json_data = {
        "key" : "value"
    }

    function add() {
        let a = document.getElementById("a").value;
        let b = document.getElementById("b").value;
        document.getElementById("result").innerHTML = (parseInt(a)+parseInt(b)).toString();
    }
    let subtract = ()=>{
        let a = document.getElementById("a").value;
        let b = document.getElementById("b").value;
        document.getElementById("result").innerHTML = (parseInt(a)-parseInt(b)).toString();
    }


</script>
</body>
</html>
